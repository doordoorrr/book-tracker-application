{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "env": {
      "NEXT_PUBLIC_API_URL": "https://your-api-url.com/",
      "NEXT_PUBLIC_FIREBASE_API_KEY": "your-firebase-api-key"
    }
  }
